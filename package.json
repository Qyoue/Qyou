{
  "name": "qyou-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*",
    "stellar/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:admin\" \"npm run dev:mobile\"",
    "dev:api": "npm run dev --workspace=apps/api",
    "dev:admin": "npm run dev --workspace=apps/admin-web",
    "dev:mobile": "npm run start --workspace=apps/mobile"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
